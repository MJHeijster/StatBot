    "Commands": {
      "Prefix": "!",
      "Exclude": "excludefromstats",
      "Include": "includeinstats",
      "Stats": {
        "Command": "stats",
        "Url": ""
      }
    }
  },
  "mIRCStats": {
    "Path": "",
    "NicksFile": "nicks.txt",
    "NickSection": "[common]",
    //Leave value empty if you don't want to use it.
    "GeneratorFile": "C:\\Discord\\Statbot\\mircstats\\Generate Stats.bat",
