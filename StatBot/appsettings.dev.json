    //Delay until the stats will be generated again.
    "LaunchEveryMinutes": 0
  },
  "Application": {
    // 	Valid options: 
    //  channelid
    //  channelname
    //  single
    "LoggingFileName": "channelid",
    "NotificationDelay": 30000,
